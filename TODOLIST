import java.util.*;
import java.lang.*;
import java.io.*;

class Codechef
{
	public static void main (String[] args) throws java.lang.Exception
	{
		// your code goes here
		Scanner sc=new Scanner(System.in);
		int t=sc.nextInt();
		
		for(int i=0;i<t;i++)
		{
		    int count=0;
		    int n=sc.nextInt();
		    for(int j=0;j<n;j++)
		    {
		    int x=sc.nextInt();
		    if(x>=1000)
		    count++;
		    
		    }
		    	System.out.println(count);
		}
	

	}
}
