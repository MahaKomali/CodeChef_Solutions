import java.util.*;
import java.lang.*;
import java.io.*;

class Codechef
{
	public static void main (String[] args) throws java.lang.Exception
	{
		// your code goes here
		int count=0;
		Scanner sc=new Scanner(System.in);
		for(int i=0;i<4;i++)
		{
		    int p=sc.nextInt();
		    if(p>=10){
		        count++;
		    }
		}
		System.out.println(count);

	}
}
